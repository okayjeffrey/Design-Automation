try	tell application "Finder"		set input to selection as Unicode text	end tell	set sketchDocument to quoted form of POSIX path of input as text	set sketchPath to quoted form of POSIX path of (do shell script "dirname " & sketchDocument & "")	set finalFolder to sketchPath & "/Previews" as text	do shell script "/usr/local/bin/sketchtool export artboards " & sketchDocument & " --output=" & finalFolder & " --formats=jpg"	--	sketchtool export artboards [Document] --output=[Location] --formats=jpgon error	display alert "Needs to be a Sketch file"end try