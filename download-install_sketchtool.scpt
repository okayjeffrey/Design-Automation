try	set hello to (do shell script "/usr/local/bin/sketchtool -v")	if hello > 1 then		display alert "Sketchtool already installed"	end ifon error	-- Prompt the user of sketchtool download	display dialog "Want to download and install the latest Sketchtool to desktop?" buttons {"cancel", "Yup Thanks"} default button 2	if result = {button returned:"Yup Thanks"} then		do shell script "cd ~/Desktop && { curl -O http://sketchtool.bohemiancoding.com/sketchtool-latest.zip;}"		try			-- Move Sketchtool Option			display dialog "Sketchtool has been downloaded can we move it to /usr/local/bin/ for you?" buttons {"No Thanks", "Yes Please!"} default button 2			--If user wants to move			if result = {button returned:"Yes Please!"} then				-- do shell script "cd ~/Desktop && open sketchtool-latest.zip;"				do shell script "open ~/Desktop/sketchtool-latest.zip && sleep 5 && mv ~/Desktop/sketchtool/sketchtool* /usr/local/bin/"				--If user does not want to move			else				display dialog "Good Luck!"			end if		on error			display dialog "There has been a problem"			#		do shell script "cd ~/Desktop && mv sketchtool/* /usr/local/bin/"		end try	end if	display dialog "All Done! get your Sketchtooling on!" buttons {"Sweet!"} default button 1end try